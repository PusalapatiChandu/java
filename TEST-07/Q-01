
class bankacc {
   public int accno;
   public int balance;
   public void getdata(int accno,int balance){
       this.accno=accno;
       this.balance=balance;
   }
   
}
 class saving extends bankacc{
    public void withdraw(int amount,int balance){
        amount=amount-balance;
        System.out.println(amount);
    }
}
 class checking extends bankacc{
    public void deposit(int balance,int amount){
        amount=balance+amount;
        System.out.println(amount);
    }
}
public class main{
    public static void main(String[] args){
        bankacc b=new bankacc();
        saving s=new saving();
        checking c=new checking();
        b.getdata(123456789,5000);
        s.withdraw(5000,1000);
        c.deposit(5000,1000);
    }
}
